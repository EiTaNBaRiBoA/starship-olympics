script/source = "extends TestChamberLog


func _ready():
	super._ready()
	Events.sth_collected.connect(_on_sth_collected)
	
func _on_sth_collected(collector, collectee):
	log_line(\"[b]%s[/b] collected [b]%s[/b] (%d points)\" % [collector.name, collectee.name, collectee.get_points()])
"

[sub_resource type="GDScript" id="GDScript_dqhjw"]
script/source = "extends Timer

@export var period := 4

func _ready():
	tick.call_deferred()

func _on_timeout():
	tick()

func tick():
	Events.beat.emit(period, wait_time)
	wait_time *= 0.99
"

